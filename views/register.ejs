<body>
  <%- include('partials/_header') %>



  <main class="listing_container">

    <form action="/register" method="post" class="registration-form">
      <div class="registration-wrapper">
        <label for="Name">Name</label>
        <input type="text" class="form-control" placeholder="Name" name="Name">
      </div>
      <div class="registration-wrapper">
        <label for="email">Email Address</label>
        <input type="email" name="email" class="form-control" aria-describedby="emailHelp" placeholder="user@example.com">
      </div>
      <div class="registration-wrapper">
        <label for="phone-number">Phone Number</label>
        <input class="form-control" placeholder="(XXX)-XXX-XXXX" type="tel" name="phone-number">
      </div>
      <div class="registration-wrapper">
        <label for="city">City</label>
        <input type="text" class="form-control" placeholder="City" name="city">
      </div>
      <div class="registration-wrapper">
        <label for="password">Password</label>
        <input type="password" class="form-control" placeholder="Password" name="password">
      </div>
      <div class="registration-wrapper">
        <label for="password">Confirm Password</label>
        <input type="password" class="form-control" placeholder="Password" name="confirm-password">
      </div>
      <div>
        <button type="submit" class="btn btn-primary">Submit</button>
      </div>
    </form>



