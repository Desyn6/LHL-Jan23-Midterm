<!DOCTYPE html>
<html lang="en">

<head>
  <title>Home Page</title>

  <meta charset="UTF-8" >
  <meta http-equiv="X-UA-Compatible" content="IE=edge" >
  <meta name="viewport" content="width=device-width, initial-scale=1.0" >

  <link rel="stylesheet" href="/vendor/normalize-4.1.1.css" />
  <link rel="stylesheet" href="/vendor/border-box.css" />
  <link rel="stylesheet" href="/styles/main.css" />
  <link rel="stylesheet" href="/styles/layout.css" />

  <script src="/vendor/jquery-3.0.0.js"></script>
  <script defer src="/scripts/app.js"></script>
  <script defer src="/scripts/users.js"></script>
</head>

<body>
  <%- include('partials/_header') %>
  <main class="listing_container">
    <form action="/api/users/search" method="post" id="search_form" class="new-listing-form">

      <div class="new-listing-wrapper">
        <label for="title">Title</label>
        <input id='title' type="text" name="title">
      </div>
      
      <div class="new-listing-wrapper">
        <label for="date-posted">Date Posted</label>
        <input id="date-posted" type="text" name="date-posted">
      </div>

      <div class="new-listing-wrapper">
        <label for="asking-price">Asking Price</label>
        <input id="asking-price" type="text" name="asking-price">
      </div>

      <button id="search_button" type="submit" >Search</button>
    </form>
  </main>
  <%- include('partials/_footer') %>
</body>

</html> 